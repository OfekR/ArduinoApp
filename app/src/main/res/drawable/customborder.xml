<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="6dp"
        android:right="6dp"
        android:bottom="10dp"
        android:top="10dp">
        <shape android:shape="oval"  android:layout_height="50dp" android:layout_width="50dp">
            <gradient
                android:angle="90"
                android:centerColor="#C7232222"
                android:centerX="0.3"
                android:centerY="0.3"
                android:endColor="#1D1E22"
                android:gradientRadius="150dp"
                android:startColor="#393A3C"
                android:type="radial"
                android:useLevel="false" />
            <stroke
                android:width="5dp"
            android:color="#9BFFFFFF" />
        </shape>
    </item>

</layer-list>
